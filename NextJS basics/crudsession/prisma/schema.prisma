generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL") 
}

model Employee {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  name     String
  role     Role   @default(EMP)
  salary   Int
}

enum Role {
  EMP
  MANAGER
}
